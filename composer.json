{
    "name": "giadc/doctrine-json-api",
    "description": "A package for handling JSON API requests with Doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "David Hill",
            "email": "dhill2@gannett.com"
        },
        {
            "name": "Ryan Marganti",
            "email": "rmarganti@gannett.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.11",
        "giadc/json-api-request": "dev-php-8-1",
        "giadc/json-api-response": "dev-php-8",
        "symfony/cache": "^6.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.4",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^8.4",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Giadc\\DoctrineJsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
