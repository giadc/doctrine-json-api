{
    "name": "giadc/doctrine-json-api",
    "description": "A package for handling JSON API requests with Doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "David Hill",
            "email": "dhill2@gannett.com"
        },
        {
            "name": "Ryan Marganti",
            "email": "rmarganti@gannett.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "doctrine/orm": "2.5.*|2.6.*|2.7.*",
        "giadc/json-api-request": "^2.0.1",
        "giadc/json-api-response": "^2.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.4",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^0.12.56",
        "phpstan/phpstan-doctrine": "^0.12.9",
        "phpstan/phpstan-mockery": "^0.12.3",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^8.4",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Giadc\\DoctrineJsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
